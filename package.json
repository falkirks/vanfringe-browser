{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "0.0.0",
  "private": true,
  "author": "Mr. Gumby <mr.gumby@example.com>",
  "copyright": "Â© 2017, Gumby inc.",
  "homepage": "http://example.com",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "node build/start.js",
    "release": "webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "datatables.net-dt": "^1.10.19",
    "datatables.net-responsive-dt": "^2.2.3",
    "datatables.net-select-dt": "^1.2.7",
    "fs-jetpack": "^2.1.0",
    "handlebars": "^4.0.11",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "electron": "2.0.8",
    "electron-builder": "^20.15.1",
    "electron-mocha": "^6.0.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
